<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <antom_alipay_cn>
                <debug>1</debug>
                <active>1</active>
                <title>Alipay</title>
                <currency>USD</currency>
                <can_use_checkout>1</can_use_checkout>
                <sort_order>1</sort_order>
                <group>offline</group>
                <info_block>Antom\Frontend\Block\Info</info_block>
            </antom_alipay_cn>
            <antom_card>
                <debug>1</debug>
                <active>1</active>
                <title>Card</title>
                <currency>USD</currency>
                <can_use_checkout>1</can_use_checkout>
                <sort_order>1</sort_order>
                <group>offline</group>
                <info_block>Antom\Frontend\Block\CardInfo</info_block>
            </antom_card>
        </payment>
    </default>
</config>
